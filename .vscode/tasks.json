{
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "args": [],
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "${workspaceRoot}/CenterForDevelopers/CenterForDevelopers.csproj"
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "Unit Tests",
            "command": "dotnet",
            "args": [
                "test"
            ],
            "options": {
                "cwd": "${workspaceRoot}/CenterForDevelopersTest"
            },
            "isTestCommand": true,
            "showOutput": "always",
            "problemMatcher": "$msCompile"
        }
    ]
}